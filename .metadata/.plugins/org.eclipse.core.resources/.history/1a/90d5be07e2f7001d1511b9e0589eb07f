package ByteBankHerdadoContas.TiposContas;

public class Conta {
	
	// Atributos
	private double saldo;
	private int agencia;
	private int numero;
	private Cliente titular;
	
	private static int numeroDeContas;
	
	//MÃ©todos
	public Conta(int agencia, int numero) {
		Conta.numeroDeContas++;
		
		this.agencia = agencia;
		this.numero = numero;
		this.saldo = 10.0;
	}
	
	

}
